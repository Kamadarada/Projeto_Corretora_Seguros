<script type="text/javascript">
let slideIndexx = 0;
showSlides2();

function showSlides2() {
  let ii;
  let slides = document.getElementsByClassName("MeusSlides2");
  for (ii = 0; ii < slides.length; ii++) {
    slides[ii].style.display = "none";
  }
  slideIndexx++;
  if (slideIndexx > slides.length) {slideIndexx = 1}
  slides[slideIndexx-1].style.display = "block";
  setTimeout(showSlides2, 10000);
}
</script>